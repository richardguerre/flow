{
  "src": "./apps/web/src",
  "schema": "./apps/web/src/relay/schema.graphql",
  "language": "typescript",
  "artifactDirectory": "./apps/web/src/relay/__generated__",
  "schemaExtensions": ["./apps/web/src/relay/local"],
  "eagerEsModules": true,
  "noFutureProofEnums": true,
  "customScalars": {
    "Date": "string",
    "DateTime": "string",
    "PositiveInt": "number",
    "JSON": "JsonValue",
    "Time": "string"
  },
  "exclude": ["**/node_modules/**", "**/__mocks__/**", "**/__generated__/**"]
}
