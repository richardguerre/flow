{
  "name": "@flowdev/plugin-github",
  "description": "Official GitHub plugin for Flow",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "build:web": "bunx --bun vite build -c vite.web.ts",
    "build:server": "bunx --bun vite build -c vite.server.ts",
    "build": "bun run build:web && bun run build:server",
    "dev:web": "bunx --bun vite build -c vite.web.ts --watch",
    "dev:server": "bunx --bun vite build -c vite.server.dev.ts --watch"
  },
  "dependencies": {
    "@octokit/core": "^5.0.1",
    "@vitejs/plugin-react": "4.1.0",
    "vite": "4.4.11"
  }
}
