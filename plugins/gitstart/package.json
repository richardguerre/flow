{
  "name": "@flowdev/plugin-gitstart",
  "description": "Official GitStart plugin for Flow",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "build:web": "vite build -c vite.web.ts",
    "build:server": "vite build -c vite.server.ts",
    "build": "bun run build:web && bun run build:server",
    "dev:web": "vite build -c vite.web.ts --watch",
    "dev:server": "vite build -c vite.server.dev.ts --watch"
  },
  "dependencies": {
    "@flowdev/plugin": "workspace:*"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.0.2"
  }
}
